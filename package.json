{
  "name": "caju-front",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "init:db": "json-server --watch db.json",
    "init:db:e2e": "json-server --watch db-e2e.json",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npm-run-all -p -r init:db:e2e dev playwright:e2e:ui",
    "playwright:e2e": "playwright test",
    "playwright:e2e:ui": "wait-on http://localhost:3001 && playwright test --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@radix-ui/react-alert-dialog": "1.1.2",
    "@tanstack/react-query": "5.59.15",
    "axios": "1.7.2",
    "json-server": "1.0.0-beta.1",
    "lottie-react": "2.4.0",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.53.0",
    "react-hot-toast": "2.4.1",
    "react-icons": "5.2.1",
    "react-loading-skeleton": "3.5.0",
    "react-router-dom": "5.1.2",
    "styled-components": "6.1.11",
    "uuid": "10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@esbuild/linux-arm64": "0.20.2",
    "@faker-js/faker": "9.0.3",
    "@nabla/vite-plugin-eslint": "2.0.4",
    "@playwright/test": "1.48.1",
    "@rollup/rollup-linux-arm64-gnu": "4.24.0",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "22.7.5",
    "@types/react": "18.2.66",
    "@types/react-beautiful-dnd": "13.1.8",
    "@types/react-dom": "18.2.22",
    "@types/react-helmet": "6.1.11",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-istanbul": "2.1.3",
    "concurrently": "9.0.1",
    "eslint": "8.57.0",
    "eslint-config-henrique": "2.0.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "happy-dom": "15.7.4",
    "npm-run-all": "4.1.5",
    "typescript": "5.2.2",
    "vite": "5.2.0",
    "vitest": "2.1.3",
    "wait-on": "8.0.1"
  }
}
